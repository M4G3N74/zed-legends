{
  "name": "music-stream-next",
  "version": "1.0.0",
  "description": "A music streaming application with auto-play functionality",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "server": "node server.js",
    "dev:server": "nodemon server.js",
    "dev:all": "concurrently \"npm run dev:server\" \"npm run dev\"",
    "start:all": "concurrently \"npm run server\" \"npm run start\"",
    "build:static": "next build && next export",
    "start:optimized": "node server-optimized.js"
  },
  "keywords": [
    "music",
    "streaming",
    "autoplay",
    "javascript",
    "nextjs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^5.3.1",
    "music-metadata": "^8.1.4",
    "next": "^14.1.0",
    "node-id3": "^0.2.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^9.0.0",
    "eslint-config-next": "^14.1.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0"
  }
}
